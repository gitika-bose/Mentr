<h1>All Mentors</h1>
<% @users.each do |user| %>
  <div class="card" style="margin: 2rem">
    <div class="card-body">
      <h5 class="card-title"><%= user.username %></h5>
      <h6 class="card-subtitle mb-2 text-muted"><%= user.email %></h6>
      <p class="card-text">Subjects taught XYZ with experience working</p>
      <a href="#" class="card-link">Contact</a>
      <a href="<%= catalog_path(user.id) %>" class="card-link">More info</a>
    </div>
  </div>
  <% end %>
